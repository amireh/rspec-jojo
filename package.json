{
  "name": "rspec-jojo",
  "version": "1.0.3",
  "description": "RSpec runner.",
  "main": "dist/JojoCLI.js",
  "bin": {
    "jojo": "./dist/JojoCLI.js"
  },
  "scripts": {
    "build": "babel --ignore '*.test.js' -d dist lib",
    "lint": "eslint lib/*.js",
    "test": "mocha --compilers js:babel-core/register 'lib/**/*.test.js'",
    "prepublish": "npm run lint && npm run test && npm run build"
  },
  "author": "Ahmad Amireh",
  "keywords": [
    "rspec",
    "tests",
    "tdd",
    "runner",
    "ruby"
  ],
  "license": "MIT",
  "repository": "amireh/rspec-jojo",
  "dependencies": {
    "blessed": "^0.1.81",
    "chokidar": "^1.4.1",
    "commander": "^2.9.0",
    "react": "^0.14.3",
    "react-blessed": "^0.1.7",
    "react-schema": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.11.3",
    "mocha": "^2.3.4",
    "mock-require": "^1.2.1",
    "sinon": "^1.17.2"
  }
}
